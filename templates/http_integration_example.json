{"name": "HTTP集成示例模板", "description": "演示如何在综合处理模板中使用HTTP集成任务类型", "version": "1.0", "parameters": {"api_url": {"type": "string", "description": "API请求URL", "default": "https://httpbin.org/get"}, "api_method": {"type": "string", "description": "HTTP请求方法", "default": "GET"}, "api_token": {"type": "string", "description": "API认证Token", "default": ""}, "query_param": {"type": "string", "description": "查询参数", "default": "test"}, "save_response": {"type": "boolean", "description": "是否保存响应到文件", "default": true}}, "tasks": [{"id": "task1", "name": "HTTP GET请求", "type": "http_integration", "params": {"method": "${api_method}", "url": "${api_url}", "query_params": {"param": "${query_param}"}, "save_response": "${save_response}", "save_filename": "api_response"}}]}